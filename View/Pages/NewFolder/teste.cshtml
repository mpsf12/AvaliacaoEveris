@page
@model View.Pages.NewFolder.testeModel

@{
    ViewData["Title"] = "teste";
}

<h2>teste</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProdutoMovimentacao[0].Movimentacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProdutoMovimentacao[0].Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProdutoMovimentacao[0].IdEmpresaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProdutoMovimentacao[0].IdProdutoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ProdutoMovimentacao) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Movimentacao)
            </td>
